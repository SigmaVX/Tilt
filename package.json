    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "formidable": "^1.2.1",
    "moment": "^2.22.2",
    "nodemon": "^1.17.5",
    "sequelize": "^4.37.10"
